{
    "description": "dusk-browser-extension",
    "manifest_version": 3,
    "name": "Dusk Browser Extension",
    "version": "1.0.0",
    "permissions": ["storage"],
    "sandbox": {
      "pages": [
        "index.html"
      ]
    },
    "action": {
      "default_popup": "index.html",
      "default_icon":{
        "16": "favicon.png"
      }
    }
  }
  


