{
    "description": "web-extension-wallet",
    "manifest_version": 3,
    "name": "DUSK WALLET TEST",
    "version": "1.0.0",
    "permissions": ["storage"],
    "background": {
      "service_worker": "build/background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon":{
        "16": "favicon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["build/content.js"]
      }
    ]
    
  }
  


