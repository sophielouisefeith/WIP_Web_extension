{
    "name": "Dusk Wallet",
    "description": "generate keys",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts":[ 
      {
        "matches":[ "<all_urls>"],
        "js": [ "content.js" ]
       
      }
    ],
    "background" :[ {
        "scripts": ["background.js"]
        }
    ],
    "permissions": ["storage", "activeTab", "tabs", "scripting"],
    "action": {
     "default_popup":"popup.html",
     "default_title":"pop up comes here",
      "default_icon": {
        "16": "/images/dusk16.png",
        "32": "/images/dusk128.png",
        "48": "/images/dusk128.png",
        "128": "/images/dusk128.png"
      }
    },
    "icons": {
      "16": "/images/dusk16.png",
      "32": "/images/dusk128.png",
      "48": "/images/dusk128.png",
      "128": "/images/dusk128.png"
    }
  }